{
  "name": "hull",
  "version": "0.14.0-beta.14",
  "description": "A Node.js client for hull.io",
  "main": "lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/hull/hull-node.git"
  },
  "homepage": "https://github.com/hull/hull-node",
  "bugs": {
    "url": "https://github.com/hull/hull-node/issues"
  },
  "engines": {
    "node": ">=6.12.0"
  },
  "author": "Romain Dardour <romain@hull.io>",
  "license": "MIT",
  "scripts": {
    "flow": "flow",
    "lint": "eslint src",
    "documentation:lint": "documentation lint src",
    "prettier": "prettier --write \"./src/**/*.{js,json}\"",
    "test": "npm run test:lint && npm run test:flow && npm run test:unit && npm run test:integration",
    "test:lint": "yarn lint && yarn documentation:lint",
    "test:modules": "npm outdated --depth=0",
    "test:unit": "NODE_ENV=test mocha --require babel-register -R spec ./test/unit/*.js ./test/unit/**/*.js",
    "test:integration": "NODE_ENV=test mocha --require babel-register -R spec ./test/integration/*.js",
    "test:flow": "flow check",
    "test:coverage": "nyc --babel-cache --hook-run-in-context false npm run test",
    "update": "updtr",
    "clean": "rimraf lib",
    "build": "npm run clean && babel src -d lib",
    "dev": "babel src -d lib -w",
    "prepublish": "npm run build",
    "documentation": "documentation build src -f md -o API.md --access public --markdownToc=false",
    "precommit": "yarn lint && yarn flow && lint-staged && npm run documentation && git add API.md"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,css,less,json}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": false,
    "arrowParens": "avoid",
    "semi": true
  },
  "dependencies": {
    "JSONStream": "^1.1.2",
    "aws-sdk": "^2.81.0",
    "babel-polyfill": "^6.26.0",
    "basic-auth": "^1.1.0",
    "batch-stream": "^0.1.3",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "bull": "^3.0.0-rc.3",
    "cache-manager": "2.6.0",
    "connect": "^3.4.1",
    "connect-timeout": "^1.8.0",
    "csv-stream": "^0.1.3",
    "datadog-metrics": "^0.4.0",
    "debug": "^3.1.0",
    "del": "^2.2.1",
    "dogapi": "^2.6.0",
    "ejs": "^2.5.6",
    "hull-client": "^2.0.0",
    "jsonwebtoken": "^7.4.3",
    "jwt-simple": "^0.5.0",
    "kue": "^0.11.5",
    "kue-ui": "^0.1.0",
    "lodash": "^4.17.5",
    "moment": "^2.22.2",
    "newrelic": "^4.1.4",
    "passport": "^0.3.2",
    "progress-bar-webpack-plugin": "^1.11.0",
    "promise-streams": "^1.0.1",
    "promisepipe": "^2.1.3",
    "raven": "^2.4.2",
    "raw-body": "^2.1.7",
    "react-hot-loader": "^4.2.0",
    "request": "^2.72.0",
    "sns-validator": "^0.3.0",
    "sqs-consumer": "^3.6.1",
    "supply": "0.0.4",
    "urijs": "^1.18.7",
    "uuid": "^3.3.2",
    "webpack": "^3.12.0",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.22.2"
  },
  "peerDependencies": {
    "express": "^4.16.3",
    "newrelic": "^4.1.4"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-flow-comments": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "documentation": "^6.3.3",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.6.2",
    "flow-bin": "^0.78.0",
    "flow-typed": "^2.4.0",
    "husky": "^0.14.3",
    "isparta": "^4.0.0",
    "lint-staged": "^7.2.0",
    "minihull": "^3.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.0",
    "nock": "^9.2.3",
    "node-mocks-http": "^1.7.0",
    "nyc": "^11.0.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.0",
    "sinon": "^2.2.0",
    "sinon-chai": "^2.10.0",
    "superagent": "^3.8.2",
    "supertest": "^3.1.0",
    "updtr": "^1.0.0"
  },
  "nodeBoilerplateOptions": {
    "mochaGlobals": [
      "stub",
      "spy",
      "expect"
    ]
  },
  "nyc": {
    "include": [
      "src"
    ]
  }
}
